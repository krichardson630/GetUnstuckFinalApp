<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logInOneVM" type="com.kamishasapplication.app.modules.loginone.data.viewmodel.LogInOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLoginOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleteal_400">
        <LinearLayout android:id="@+id/linearColumnunstuck"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_41pxv"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtUnstuck"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{logInOneVM.logInOneModel.txtUnstuck}"
                style="@style/txtAcmeregular40" tools:text="@string/lbl_unstuck"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTitletext"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{logInOneVM.logInOneModel.txtTitletext}"
                style="@style/txtInterregular32" tools:text="@string/lbl_welcome_back"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnEmailusername"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_58pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_email_username"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_email_username"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_password"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_password"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtGroupSix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_1pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_8pxv"
                android:singleLine="true"
                android:text="@{logInOneVM.logInOneModel.txtGroupSix}"
                style="@style/txtRoundedOutline" tools:text="@string/lbl_login"/>
            <LinearLayout android:id="@+id/linearRowlineone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_58pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/white_A700"
                    android:id="@+id/lineLineOne"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.17" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtORLOGINWITH"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInOneVM.logInOneModel.txtORLOGINWITH}"
                    style="@style/txtRobotoromanbold32" tools:text="@string/lbl_or_login_with"/>
                <View android:background="@color/white_A700"
                    android:id="@+id/lineLineTwo"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.14" android:layout_width="0dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowgoogleone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                <ImageView android:id="@+id/imageGoogleOne"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_google1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_google1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithGo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_89pxh"
                    android:layout_marginStart="@dimen/_61pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInOneVM.logInOneModel.txtSigninwithGo}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_go"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowfacebookone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                <ImageView android:id="@+id/imageFacebookOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSigninwithFa"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_79pxh"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInOneVM.logInOneModel.txtSigninwithFa}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_in_with_fa"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowconfirmation"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInOneVM.logInOneModel.txtConfirmation}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_don_t_have_an_a"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignup"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInOneVM.logInOneModel.txtSignup}"
                    style="@style/txtPoppinsregular14_2" tools:text="@string/lbl_sign_up"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
